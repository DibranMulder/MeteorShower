<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dibranmulder.meteorshower" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <allow-intent href="market:*" />
    <name>MeteorShower</name>
    <description>
        A basic WebGL game where the user has to dodge incoming meteors
    </description>
    <author email="dibran.mulder@outlook.com" href="http://dibranmulder.nl">
        Dibran Mulder
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
</widget>
